    "deviceEventCategory" : "stix:Observables",
    "deviceEventClassId"  : "AddressObj:AddressObjectType"
  },
  "extract_mappings" : [
    {
      "type"   : "xpath",
      "origin" : "@id",
      "target" : "externalId"
    },
    {
      "type"   : "xpath",
      "origin" : "descendant::AddressObj:Address_Value/text()",
      "target" : "sourceAddress"
    }
  ]
}
